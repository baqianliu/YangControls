		<GradientStop o:Freeze="True" Color="{DynamicResource PrimaryColor}" Offset="0"/>
		<GradientStop o:Freeze="True" Color="{DynamicResource PrimaryToolColor}" Offset="1"/>
	</LinearGradientBrush>
